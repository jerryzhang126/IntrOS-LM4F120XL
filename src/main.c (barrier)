#include <lm4f_120xl.h>
#include <os.h>

bar_t bar = BAR_INIT(2);

void slave()
{
	bar_wait(&bar);

	LED_Tick();
}

void master()
{
	tsk_delay(SEC);

	bar_wait(&bar);
}

tsk_t sla = TSK_INIT(slave);
tsk_t mas = TSK_INIT(master);

int main()
{
	LED_Config();

	sys_init();
	tsk_start(&sla);
	tsk_start(&mas);
	tsk_stop();
}
